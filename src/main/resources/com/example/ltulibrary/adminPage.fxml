<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.ltulibrary.adminPageController">
   <children>
      <BorderPane prefHeight="400.0" prefWidth="231.0" style="-fx-background-color: #263F70;">
         <top>
            <Text fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="Admin Page" BorderPane.alignment="CENTER">
               <font>
                  <Font size="36.0" />
               </font>
            </Text>
         </top></BorderPane>
      <Button fx:id="bookButton" layoutX="333.0" layoutY="73.0" mnemonicParsing="false" onAction="#onBook" prefHeight="56.0" prefWidth="129.0" style="-fx-background-color: green;" text="Book">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Button>
      <Button fx:id="DvdButton" layoutX="333.0" layoutY="172.0" mnemonicParsing="false" onAction="#onDvd" prefHeight="56.0" prefWidth="129.0" style="-fx-background-color: green;" text="DVD">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Button>
      <Button fx:id="adminReturnButton" layoutX="355.0" layoutY="330.0" mnemonicParsing="false" onAction="#OnAdminReturn" prefHeight="42.0" prefWidth="84.0" text="Return" />
      <Text layoutX="251.0" layoutY="43.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Customize book or dvd in the database">
         <font>
            <Font size="18.0" />
         </font>
      </Text>
   </children>
</AnchorPane>
